{
    "name": "rentCarApp",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "ios": "react-native run-ios --simulator='iPhone 14 Pro'",
        "android": "react-native run-android",
        "start": "react-native start",
        "test": "jest",
        "reset": "yarn start --reset-cache",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "keygen": "keytool -genkeypair -v -keystore android/app/getride-key.keystore -alias getride -keyalg RSA -keysize 2048 -validity 10000",
        "android:clean": "cd android && gradlew clean && cd ..",
        "android:build-dev": "cd android && gradlew assembleReleaseStaging && cd ..",
        "android:build-prod": "cd android && gradlew assembleRelease && cd ..",
        "android:dev": "react-native run-android --variant=developmentdebug",
        "android:dev-release": "react-native run-android --variant=developmentrelease",
        "android:prod": "react-native run-android --variant=productiondebug",
        "android:prod-release": "react-native run-android --variant=productionrelease",
        "appcenter:android-dev": "appcenter codepush release-react -a antoni_luthfi3/Get-Ride-User-Android -d Development -m --disable-duplicate-release-error",
        "appcenter:android-prod": "appcenter codepush release-react -a antoni_luthfi3/Get-Ride-User-Android -d Production -m --disable-duplicate-release-error",
        "appcenter:ios-dev": "appcenter codepush release-react -a antoni_luthfi3/Get-Ride-User-iOS -d Development -m --disable-duplicate-release-error",
        "appcenter:ios-prod": "appcenter codepush release-react -a antoni_luthfi3/Get-Ride-User-iOS -d Production -m --disable-duplicate-release-error"
    },
    "dependencies": {
        "@gorhom/bottom-sheet": "^4",
        "@react-native-async-storage/async-storage": "^1.17.11",
        "@react-native-clipboard/clipboard": "^1.11.1",
        "@react-native-community/progress-bar-android": "^1.0.5",
        "@react-navigation/bottom-tabs": "^6.4.0",
        "@react-navigation/native": "^6.0.13",
        "@react-navigation/native-stack": "^6.9.1",
        "@react-navigation/stack": "^6.3.2",
        "@reduxjs/toolkit": "^1.8.6",
        "apisauce": "^2.1.6",
        "date-fns": "^2.29.3",
        "i18next": "^22.4.9",
        "moment": "^2.29.4",
        "react": "18.1.0",
        "react-i18next": "^12.1.5",
        "react-native": "0.70.3",
        "react-native-code-push": "^7.1.0",
        "react-native-collapsible": "^1.6.0",
        "react-native-config": "^1.5.0",
        "react-native-country-flag": "^1.1.9",
        "react-native-device-info": "^10.3.0",
        "react-native-dotenv": "^3.4.2",
        "react-native-gesture-handler": "^2.7.1",
        "react-native-image-picker": "^4.10.2",
        "react-native-localize": "^2.2.4",
        "react-native-modern-datepicker": "^1.0.0-beta.91",
        "react-native-otp-inputs": "^7.4.0",
        "react-native-qrcode-svg": "^6.1.2",
        "react-native-reanimated": "^2.11.0",
        "react-native-reanimated-carousel": "^3.1.5",
        "react-native-redash": "^18.1.0",
        "react-native-safe-area-context": "^4.4.1",
        "react-native-screens": "^3.18.2",
        "react-native-svg": "^13.6.0",
        "react-native-vector-icons": "^9.2.0",
        "react-native-wheel-scroll-picker": "^0.2.4",
        "react-native-wheel-scrollview-picker": "^2.0.1",
        "react-redux": "^8.0.4",
        "redux-persist": "^6.0.0",
        "styled-components": "^5.3.6"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/runtime": "^7.12.5",
        "@react-native-community/eslint-config": "^2.0.0",
        "@tsconfig/react-native": "^2.0.2",
        "@types/jest": "^26.0.23",
        "@types/react": "^18.0.21",
        "@types/react-native": "^0.70.4",
        "@types/react-native-dotenv": "^0.2.0",
        "@types/react-native-modern-datepicker": "^1.0.1",
        "@types/react-native-vector-icons": "^6.4.12",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.37.0",
        "@typescript-eslint/parser": "^5.37.0",
        "babel-jest": "^26.6.3",
        "babel-plugin-module-resolver": "^4.1.0",
        "eslint": "^7.32.0",
        "jest": "^26.6.3",
        "metro-react-native-babel-preset": "0.72.3",
        "react-test-renderer": "18.1.0",
        "typescript": "^4.8.3"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
